#!/bin/bash
#PBS -N argonnite-2014-07-10-2
#PBS -A CI-DEB000002
#PBS -l walltime=2:00:00
#PBS -l mppwidth=384

# 16 nodes, 24 threads per node
cd $PBS_O_WORKDIR

aprun -n 16 -N 1 -d 24 \
              argonnite -threads-per-node all -print-load -print-memory-usage \
              -k 43 Iowa_Continuous_Corn/GPIC.1424-1.1371.fastq > argonnite-2014-07-10-2.stdout

